{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "liquidsoap",
      "camelcase": "Liquidsoap",
      "title": "Liquidsoap",
      "scope": "source.liquidsoap",
      "file-types": [
        "liquidsoap"
      ],
      "injection-regex": "^liquidsoap$",
      "class-name": "TreeSitterLiquidsoap"
    }
  ],
  "metadata": {
    "version": "1.1.3",
    "license": "MIT",
    "description": "Liquidsoap files parser",
    "authors": [
      {
        "name": "Romain Beauxis",
        "email": "romain.beauxis@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/savonet/tree-sitter-liquidsoap"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
